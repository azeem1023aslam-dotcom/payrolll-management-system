{
  "name": "@payroll-system/payroll-system-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@payroll-system/payroll-system"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/payroll-system-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@payroll-system/payroll-system:build",
          "@payroll-system/payroll-system:serve"
        ]
      }
    }
  }
}
